@model IEnumerable<WebBiopelagos.Web.Models.AnalysisBase>

@{
    ViewData["Title"] = "Analyses";
}

<h2>@ViewData["Title"]</h2>
<p><a asp-area="" asp-controller="Home" asp-action="Index">Retour vers l'accueil</a></p>
<h3>Campagne Wallalis</h3>

<p>
    @*<a asp-action="Create">Create New</a>*@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnalysisDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocationDetails)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TmpId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalyzedByOld)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LaboratoryIdOld)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalyserStaff)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalyserStaffId2Navigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalyserStaffId3Navigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalysisSampleType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnalysisType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LaboratoryLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SampleBase)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AnalysisDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationDetails)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalyzedByOld)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LaboratoryIdOld)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalyserStaff.CountryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalyserStaffId2Navigation.CountryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalyserStaffId3Navigation.CountryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalysisSampleType.AnalysisTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnalysisType.AnalysisTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LaboratoryLocation.CountryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SampleBase.SampleNo)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.AnalysisBaseId">Details</a>
                @*<a asp-action="Edit" asp-route-id="@item.AnalysisBaseId">Edit</a>
                <a asp-action="Delete" asp-route-id="@item.AnalysisBaseId">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
